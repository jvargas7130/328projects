
public class Prog1 {
	
	public static void main(String[] args ){
		
	
	System.out.print(fib(8) );
	
	}
	
	
	public static int fib(int num){
		
		int sum = 0;
		
				
		if(num <= 1){
		
			return num;
		}

		
		int f = fib(num - 1) + fib(num - 2) ; 
		
	    int k = f+fib(num - 2) + fib(num - 3);
		
		return k;
	}
	
	
}
