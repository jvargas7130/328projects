
public class Prog1 {
	
	public static void main(String[] args ){
		
	
		System.out.println(fib(7, 0)); 
	
	}
	
	
	public static int fib(int num, int sum){
		
	
		
				
		if(num <= 1){
		
			return sum;
		}

	
		int f = sum + fib(num - 1, sum +1) + fib(num - 2, sum + 1) ; 
		
	 
		
		return sum;
	}
	
	
}
