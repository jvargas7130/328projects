
public class Prog1 {

	public static void main(String[] args) {
		int sum = 0;
		for (int i = 1; i <= 3; i++) {

			sum =+ fib(i);
			
			System.out.println(sum);
		}
		//System.out.println(sum);
	}

	

	public static int fib(int n) {

		if (1 == n || 2 == n)

		{

			return 1;

		}

		else

		{

			return fib(n - 1) + fib(n - 2);

		}

	}

}
